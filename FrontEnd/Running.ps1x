$($core[1])
$($core[2])
$($core[3])
$($core[4])
$($core[5])
$($core[6])
$($core[7])                 
$($Validation.Files_Result)
$($Validation.Karbon_Result)
$($Validation.Calm_Result)
$($Validation.ERA_Result)
$($Validation.Objects_Result)
"
