  $Validation = Invoke-Sqlcmd -ServerInstance $SQLInstance -Query "SELECT TOP 1 * FROM [$($SQLDatabase)].[dbo].$($SQLDataValidationTableName) WHERE QueueUUID='$($item.QueueUUID)';" 
